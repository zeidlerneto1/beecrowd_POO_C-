#include <iostream>
#include <iomanip>
#include <string>
 
using namespace std;

class TTurma {
    private:
        int n;
        double media[100];
        double n1[100], n2[100];
        string nome[100];
    public:
        void ler();
        void listar();
        void Media();
};

void TTurma::ler(){
    cin >> n;
    for(int i = 0; i < n; i++) {
        cin.ignore();
        getline(cin, nome[i]);
        cin >> n1[i] >> n2[i];
    }
}

void TTurma::Media(){
   for(int i = 0; i < n; i++) {
        media[i] = (n1[i] + n2[i]*2) / 3;
   }
}

void TTurma::listar(){
    cout << setprecision(2) << fixed;
    for(int i = 0; i < n; i++){
        cout << setw(50) << left << nome[i];
        cout << setw(6) << right << media[i];
        if(media[i] >= 6.0) cout << " Aprovado" << endl;
        else cout << " Reprovado" << endl;
    }
}
 
int main(){
    
    TTurma t;
	t.ler();
	t.Media();
    t.listar();
    
    return 0;
}
