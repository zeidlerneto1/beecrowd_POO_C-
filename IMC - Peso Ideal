#include <iostream>
#include <iomanip>
#include <string>

using namespace std;

#define max_pessoas 100

class Pessoa {
public:
    Pessoa();
    Pessoa(string nome, char sexo, double altura);
    string getNome() ;
    double getPeso() ;
    double calcPeso() ;

private:
    string nome;
    char sexo;
    double altura;
    double peso;
};

class CadastroPessoas {
public:
    void cadastrarPessoa(Pessoa pessoa);
    void buscarPessoa(string nome) ;

private:
    Pessoa pessoas[max_pessoas];
    int nPessoas = 0;
};

Pessoa::Pessoa() {
    this->nome = "";
    this->sexo = ' ';
    this->altura = 0.0;
    this->peso = 0.0;
}

Pessoa::Pessoa(string nome, char sexo, double altura) {
    this->nome = nome;
    this->sexo = sexo;
    this->altura = altura;
    this->peso = calcPeso();
}

string Pessoa::getNome()  {
    return nome;
}

double Pessoa::getPeso()  {
    return peso;
}

double Pessoa::calcPeso()  {
    if (sexo == 'M') {
        return 72.7 * altura - 58;
    } else {
        return 62.1 * altura - 44.7;
    }
}

void CadastroPessoas::cadastrarPessoa(Pessoa pessoa) {
    if (nPessoas < max_pessoas) {
        pessoas[nPessoas] = pessoa;
        nPessoas++;
    }
}

void CadastroPessoas::buscarPessoa(string nome)  {
    for (int i = 0; i < nPessoas; i++) {
        if (pessoas[i].getNome() == nome) {
            cout <<fixed<< setprecision(2);
            cout << pessoas[i].getNome() << " - " << pessoas[i].getPeso()<< endl;
            return;
        }
    }
cout << nome << " - nao encontrado!" << endl;
}

int main() {
CadastroPessoas cadastro;
int ler, pesquisa;
cin >> ler >> pesquisa;
for (int i = 0; i < ler; i++){
    string nome;
    char sexo;
    double altura;
    cin >> nome >> sexo >> altura;
    Pessoa pessoa(nome, sexo, altura);
    cadastro.cadastrarPessoa(pessoa);
}
for (int i = 0; i < pesquisa; i++){
    string nome;
    cin >> nome;
    cadastro.buscarPessoa(nome);
}
    return 0;
}
