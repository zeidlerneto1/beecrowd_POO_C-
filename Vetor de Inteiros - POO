#include<iostream>

using namespace std;

class TVetInt
{
    private:
        int vet[100];
        int n;
    public:
        void ler();
        void inverter();
        void listar();
};

void TVetInt::ler()
{
    cin >> n;
    for(int i=0;i<n;i++)
       cin >> vet[i];
}
void TVetInt::inverter()
{
    for (int i=0,j=n-1;i<j;i++,j--)
    {
        int aux = vet[i];
        vet[i] = vet[j];
        vet[j] = aux;
    }
}
void TVetInt::listar()
{
    cout << vet[0];
    for(int i=1;i<n;i++)
       cout << " " << vet[i];
    cout << endl;
}


int main()
{
    TVetInt v;
    v.ler();
    v.inverter();
    v.listar();
    return 0;
}
